.container
{
	background-color: #0F0F0F;
	align-items: center;
	flex: 1;
}

.win_label 
{
	margin: 10%;
	font-size: 200px;
	color: #F03030;
}

.menu_button 
{
	border-width: 5px;
	border-color: #666666;
	border-radius: 20px;
	background-color: #AF2F2F;
	color: #602020;
	font-size: 50px;
	width: 22.5%;
	transition-duration: 0.5s;
	margin-bottom: 80px;
}

.menu_button:hover
{
	background-color: #602020;
	border-radius: 40px;
	color: #200505;
}

.quit_button
{
	width: 11.25% !important;
}